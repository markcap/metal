<%= error_messages_for :concert %>
<% form_for @concert do |f| %>
  <p>
    <%= f.label :concert_date %><br />
    <%= f.date_select :concert_date %>
  </p>
  <p>
    <%= f.label :bands %><br />
    <%= f.text_field :bands %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :venue_id %><br />
    <%= f.text_field :venue_id %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
